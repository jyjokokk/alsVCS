{
  "name": "alsvcs",
  "description": "Express API for Ableton Live Set Version Control System",
  "engines": {
    "node": ">=22.17.0"
  },
  "volta": {
    "node": "22.17.0",
    "yarn": "1.22.22"
  },
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "prepare": "yarn prebuild && yarn build",
    "dev": "nodemon src/app.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch:cov": "jest --watch --coverage",
    "start": "yarn prepare && node dist/app.js",
    "format": "prettier --write src/**/*.{ts,js,json} && eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}
